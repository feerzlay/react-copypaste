    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^6.9.2",
    "react-i18next": "^11.7.3",
    "react-router-dom": "^5.2.0",
    "react-ssr-prepass": "^1.2.1",
    "react-use-resource": "^0.3.0",
    "source-map-support": "^0.5.19",
    "terser-webpack-plugin": "^4.2.3",
