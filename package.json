{
  "name": "react-copypaste",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "webpack-dev-server --config ./config/webpack.config.client.js --progress --hot",
    "serve:client": "ws --directory ./public/client --port 8080 --spa --compress",
    "serve:server": "node -r source-map-support/register public/server/server.js",
    "build": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.client.js",
    "build:client:analyze": "cross-env NODE_ENV=production BUILD_ANALYZE=1 webpack --config ./config/webpack.config.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.server.js",
    "lint": "yarn lint:eslint && yarn lint:stylelint",
    "lint:eslint": "eslint source/**/*.{ts,tsx}",
    "lint:eslint:fix": "eslint source/**/*.{ts,tsx} --fix",
    "lint:stylelint": "stylelint source/**/*.{ts,tsx}",
    "lint:stylelint:fix": "stylelint source/**/*.{ts,tsx} --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/feerzlay/react-copypaste.git"
  },
  "author": "Denis Yakshov",
